import java.util.*;
class TestClass
{
    public static void main(String args[] )
    {
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int[] arr=new int[n];
for(int i=0;i<arr.length;i++){
    arr[i]=s.nextInt();
}
long prod=1;
int mod=(int)Math.pow(10,9)+7;
// int mod=1000000007;
for(int i=0;i<arr.length;i++){
prod=(prod*arr[i])%mod;
}
System.out.println(prod);

    }

}
